{
  "name": "ngxsmk-datepicker",
  "version": "1.9.18",
  "description": "A lightweight, customizable, and easy-to-use datepicker and date range picker for Angular applications.",
  "license": "MIT",
  "engines": {
    "node": ">=20.19.0 || >=22.12.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "ng": "npx ng",
    "start": "npx ng serve demo-app",
    "build": "npx ng build",
    "build:demo": "npx ng build demo-app --configuration production",
    "build:optimized": "npx ng build ngxsmk-datepicker --configuration production && node scripts/remove-sourcemaps.js",
    "build:analyze": "node scripts/analyze-bundle.js",
    "optimize": "node scripts/optimize-build.js",
    "watch": "npx ng build --watch --configuration development",
    "test": "npx ng test ngxsmk-datepicker",
    "demo": "npx ng serve demo-app",
    "lint": "npx ng lint",
    "deploy": "npm run build:demo && npx gh-pages -d dist/demo-app/browser",
    "prepublishOnly": "npm run build:optimized",
    "prepublish:copy-license": "node -e \"require('fs').copyFileSync('LICENSE', 'dist/ngxsmk-datepicker/LICENSE')\"",
    "publish:lib": "npm run build:optimized && npm run prepublish:copy-license",
    "unpublish:version": "node scripts/unpublish-version.js",
    "publish:patch": "npm run build:optimized && npm run prepublish:copy-license && cd dist/ngxsmk-datepicker && npm publish --tag latest",
    "semantic-release": "semantic-release",
    "docs": "typedoc --options typedoc.json && node scripts/enhance-api-docs.js",
    "docs:serve": "npm run docs && npx http-server docs/api -p 8080 -o",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "@angular/common": ">=17.0.0 <24.0.0",
    "@angular/core": ">=17.0.0 <24.0.0",
    "@angular/forms": ">=17.0.0 <24.0.0",
    "luxon": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "zone.js": {
      "optional": true
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "main": "dist/ngxsmk-datepicker/fesm2022/ngxsmk-datepicker.mjs",
  "module": "dist/ngxsmk-datepicker/fesm2022/ngxsmk-datepicker.mjs",
  "es2022": "dist/ngxsmk-datepicker/fesm2022/ngxsmk-datepicker.mjs",
  "esm2022": "dist/ngxsmk-datepicker/fesm2022/ngxsmk-datepicker.mjs",
  "types": "dist/ngxsmk-datepicker/index.d.ts",
  "typings": "dist/ngxsmk-datepicker/index.d.ts",
  "homepage": "https://github.com/NGXSMK/ngxsmk-datepicker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NGXSMK/ngxsmk-datepicker.git"
  },
  "bugs": {
    "url": "https://github.com/NGXSMK/ngxsmk-datepicker/issues"
  },
  "author": {
    "name": "Sachin Dilshan",
    "url": "https://www.linkedin.com/in/sachindilshan/"
  },
  "sideEffects": false,
  "files": [
    "dist/ngxsmk-datepicker"
  ],
  "keywords": [
    "angular",
    "datepicker",
    "date-range-picker",
    "calendar",
    "standalone-component",
    "angular-17",
    "angular-18",
    "angular-19",
    "angular-20",
    "angular-21",
    "typescript",
    "date-selection",
    "i18n",
    "localization",
    "signal-forms",
    "SSR",
    "server-side-rendering",
    "angular-universal",
    "zoneless",
    "lightweight",
    "customizable",
    "open-source",
    "MIT",
    "date-picker",
    "time-picker",
    "date-time-picker",
    "holiday-calendar",
    "accessibility",
    "a11y",
    "responsive",
    "mobile-friendly",
    "touch-gestures",
    "keyboard-navigation",
    "RTL",
    "right-to-left",
    "theming",
    "dark-mode",
    "light-mode"
  ],
  "exports": {
    ".": {
      "types": "./dist/ngxsmk-datepicker/index.d.ts",
      "import": "./dist/ngxsmk-datepicker/fesm2022/ngxsmk-datepicker.mjs",
      "default": "./dist/ngxsmk-datepicker/fesm2022/ngxsmk-datepicker.mjs"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "devDependencies": {
    "@angular/common": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular-devkit/build-angular": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/jasmine": "^5.1.12",
    "angular-eslint": "^20.6.0",
    "eslint": "^9.39.0",
    "jasmine-core": "^5.12.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^21.0.0",
    "semantic-release": "^23.0.0",
    "typedoc": "^0.26.11",
    "typescript": "^5.7.0",
    "typescript-eslint": "8.46.4",
    "zone.js": "^0.15.0",
    "@storybook/angular": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-controls": "^8.6.14",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "storybook": "^8.6.14",
    "luxon": "^3.4.4",
    "@types/luxon": "^3.3.7"
  }
}